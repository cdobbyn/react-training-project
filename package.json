  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "https://github.com/LANtasy/webbro-frontend.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --env=dev",
